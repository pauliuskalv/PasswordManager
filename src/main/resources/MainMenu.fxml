<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="373.0"
            prefWidth="587.0"
            xmlns="http://javafx.com/javafx/8.0.141"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="UI.Controller.MainMenu">
   <children>
      <Button fx:id="newEntryButton"
              layoutX="14.0"
              layoutY="14.0"
              mnemonicParsing="false"
              onAction="#handleNewEntryButton"
              text="Create new entry" />
      <ListView fx:id="entries"
                layoutX="14.0"
                layoutY="52.0"
                prefHeight="305.0"
                prefWidth="285.0" />
      <VBox layoutX="323.0"
            layoutY="27.0"
            prefHeight="296.0"
            prefWidth="250.0" />
      <Button fx:id="exitButton"
              layoutX="478.0"
              layoutY="327.0"
              mnemonicParsing="false"
              onAction="#handleExitButton"
              text="Save and exit" />
      <Button fx:id="optionsButton"
              layoutX="237.0"
              layoutY="14.0"
              mnemonicParsing="false"
              onAction="#handleOptionsButton"
              text="Options" />
      <Button fx:id="deleteButton"
              layoutX="361.0"
              layoutY="327.0"
              mnemonicParsing="false"
              text="Delete entry"
              onAction="#handleDeleteButton"
      />
   </children>
</AnchorPane>
